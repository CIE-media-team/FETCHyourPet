<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sigm.fetchyourpet">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ViewMatches"

            >



        </activity>

        <activity
            android:name=".AddDog"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RescueDashboard"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AdopterDashboard"
            android:screenOrientation="portrait" />
        <activity
            android:name=".QuizActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".SignUpActivityAdopter"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SignUpAccountType"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CollectionEnlarge"
            android:parentActivityName=".Collection"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Collection"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SignInActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SignUpActivityRescue"
            android:screenOrientation="portrait" />
    </application>

</manifest>